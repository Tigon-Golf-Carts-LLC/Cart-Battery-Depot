<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="https://cartbatterydepot.com/favicon.png"/>
      <square150x150logo src="https://cartbatterydepot.com/favicon.png"/>
      <square310x310logo src="https://cartbatterydepot.com/icon-512x512.png"/>
      <wide310x150logo src="https://cartbatterydepot.com/favicon.png"/>
      <TileColor>#8cc24e</TileColor>
    </tile>
    <badge>
      <polling-uri src="https://cartbatterydepot.com/api/badge-status"/>
      <frequency>30</frequency>
    </badge>
    <notification>
      <polling-uri src="https://cartbatterydepot.com/api/notifications"/>
      <frequency>60</frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
